{
    "brackets": [],
    "colorizedBracketPairs": [],
    "autoClosingPairs": [
        [ "$[", "" ],
        [ "[", "]" ],
        [ "#:", "$0 #<" ]
    ],
    "surroundingPairs": [
        [ "[", "]" ],
    ],
    "indentationRules": {
        "increaseIndentPattern": "^.*((?<!\\$)\\[|#:[0-9]+).*$",
        "decreaseIndentPattern": "^.*((?<!\\$)\\]|#<).*$",
        "unIndentedLinePattern": "^\\s*$"
    },
    "onEnterRules": [
        {
            "beforeText": "\\$\\[([^\\[]*)$",
            "action": {
                "indent": "none"
            }
        },
        {
            "beforeText": "^.*\\[[^\\]]*$",
            "afterText": "^.*\\].*$",
            "action": {
                "indent": "indentOutdent"
            }
        },
        {
            "beforeText": "^.*\\[[^\\]]*$",
            "action": {
                "indent": "indent"
            }
        },
        {
            "beforeText": "^.*#:[0-9]+$",
            "afterText": "#<.*$",
            "action": {
                "indent": "indentOutdent"
            }
        },
        {
            "beforeText": "^.*#:[0-9]+$",
            "action": {
                "indent": "indent"
            }
        }
    ]
}