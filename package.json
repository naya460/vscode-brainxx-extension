{
    "name": "brainxx",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.80.1"
    },
    "main": "./extension.js",
    "activationEvents": [
        "onLanguage:brainxx"
    ],
    "contributes": {
        "languages": [{
            "id": "brainxx",
            "extensions": [
                ".xx"
            ]
        }],
        "grammars": [{
            "language": "brainxx",
            "scopeName": "source.brainxx",
            "path": "./brainxx.tmLanguage.json"
        }]
    }
}