{
    "name": "brainxx",
    "version": "0.2.3",
    "engines": {
        "vscode": "^1.80.1"
    },
    "main": "./extension.js",
    "contributes": {
        "languages": [{
            "id": "brainxx",
            "extensions": [
                ".xx"
            ],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "brainxx",
            "scopeName": "source.brainxx",
            "path": "./brainxx.tmLanguage.json",
            "balancedBracketScopes": [
                ""
            ]
        }]
    }
}